    "from tkinter import ttk\n",
    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "ui = ttk.Frame(root, padding=100)\n",
    "ui.grid()\n",
    "ttk.Label(ui, text=\"Hi I am Rahat.\").grid(column=0, row=0)\n",
    "ttk.Button(ui, text=\"Quit\", command=root.destroy).grid(column=1, row=0)\n",
    "root.mainloop()"
   ]
